<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PackageProfiles.ProfileTallerista.ActivitySelectBadge">

    <include
        android:id="@+id/toolbar_top_level"
        layout="@layout/toolbar"/>

    <Button
        android:id="@+id/ButtonBadge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Esta es una insignia"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_top_level" />


</android.support.constraint.ConstraintLayout>